



.PHONY: all
all: md html
	echo "Building doc finished."

.PHONY: help
help:
	@echo "Supported make targets:"
	@echo " help (this message)"
	@echo " md (generate markdown documentation)"
	@echo " html (generate HTML documentation)"
	@echo " clean (clean up markdown and HTML artifacts)"

.PHONY: md
md: main.md


.PHONY: html
html:

.PHONY: clean
clean:
	rm -f
